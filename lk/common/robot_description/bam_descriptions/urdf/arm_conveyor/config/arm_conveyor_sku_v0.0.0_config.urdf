      <mass value="0.0001"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.001" ixy="0.00001" ixz="0.00001" iyy="0.001" iyz="0.00001" izz="0.001"/>
    </inertial>
  </link>
  <!-- Front roller (positioned so outer edge aligns with conveyor length) -->
  <joint name="conveyor_surface_center_to_roller_front" type="fixed">
    <origin rpy="0 0 0" xyz="0 0.725 -0.025"/>
    <parent link="conveyor_surface_center"/>
    <child link="conveyor_roller_front"/>
  </joint>
  <link name="conveyor_roller_front">
